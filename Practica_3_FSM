};
int a = 100;
void setup()
{
  Salidas(0B000000);
  delay(100);
  display.begin(SSD1306_SWITCHCAPVCC);
  pinMode(12 , INPUT_PULLUP);
  pinMode(13 , INPUT_PULLUP);
  pinMode(35 , INPUT);
  pinMode(34 , INPUT);
  pinMode(15 , INPUT_PULLUP);
  pinMode(4  , INPUT_PULLUP);
  pinMode(39 , INPUT);
  pinMode(36 , INPUT);
  Serial.begin(9600);
  for (int i = 0; i < 6; i++)
  {
    pinMode(SALIDAS_PUERTO[i], OUTPUT);
  }
  display.clearDisplay();
  display.drawBitmap(0  , 10, ABB_MONO, 128, 64, WHITE);
  display.drawBitmap(30 , 5, ABB,      128, 64, WHITE);
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(6, 0);
  display.println("-MICROCONTROLADORES-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 10);
  display.println("-HUITZILIHUITL-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(1000);
  Salidas(0B110011);
  display.clearDisplay();
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 0);
  display.println("-JESUS_EMMANUEL-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 10);
  display.println("-JORGE_ALBERTO-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(1000);
  Salidas(0B111111);
  display.clearDisplay();
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(25, 0);
  display.println("-PRACTICA_3-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(25, 10);
  display.println("-MAQ.EDOS-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(1000);
  Salidas(0B110011);
  display.clearDisplay();
  display.display();
}

void loop()
{
S11:
  delay(a);
S1:
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S1-");
  display.drawCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.drawCircle(90, 28, 7, WHITE);
  display.drawCircle(40, 28, 7, WHITE);
  display.drawCircle(50, 10, 7, WHITE);
  display.fillCircle(80, 10, 7, WHITE);
  display.display();
  delay(10);
  Salidas(0B000001);
  // Imprimir la imagen en la pantalla
//  if ( digitalRead(12) == 0) {
//    delay(a);
//    goto S2;
//  }
  if ( digitalRead(13) == 0) { // boton 1
    delay(a);
    goto S22;
  }
  if ( digitalRead(35) == 0) { // boton 2
    goto S33;
  }
  if ( digitalRead(34) == 0) { // boton 3
    goto S55;
  }
  if ( digitalRead(15) == 0) { // boton 4
    goto S66;
  }
  if ( digitalRead(4)  == 0) { // boton retorno
    goto S66;
  }
  goto S1;
 
 S22:
  delay(a); 
 S2:

  Salidas(0B000010);
  // Imprimir la imagen en la pantalla
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S2-");
  display.drawCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.drawCircle(90, 28, 7, WHITE);
  display.drawCircle(40, 28, 7, WHITE);
  display.fillCircle(50, 10, 7, WHITE);
  display.drawCircle(80, 10, 7, WHITE);
  display.display();
  if ( digitalRead(12) == 0) {
    goto S33;
  }
  if ( digitalRead(13) == 0) {
    delay(a);
    goto S33;
  }
  if ( digitalRead(35) == 0) {
    goto S55;
  }
  if ( digitalRead(34) == 0) {
    goto S66;
  }
  if ( digitalRead(15) == 0) {
    goto S2;
  }
  if ( digitalRead(4)  == 0) {
    goto S11;
  }
  goto S2;
S33:
delay(a);
S3:
  Salidas(0B000100);
  // Imprimir la imagen en la pantalla
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S3-");
  display.drawCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.drawCircle(90, 28, 7, WHITE);
  display.fillCircle(40, 28, 7, WHITE);
  display.drawCircle(50, 10, 7, WHITE);
  display.drawCircle(80, 10, 7, WHITE);
  display.display();
  if ( digitalRead(12) == 0) {
    goto S4;
  }
  if ( digitalRead(13) == 0) {
    delay(a);
    goto S44;
  }
  if ( digitalRead(35) == 0) {
    delay(a);
    goto S6;
  }
  if ( digitalRead(34) == 0) {
    delay(a);
    goto S3;
  }
  if ( digitalRead(15) == 0) {
    goto S11;
  }
  if ( digitalRead(4)  == 0) {
    goto S22;
  }
  goto S3;
S44:
delay(a);
S4:
  Salidas(0B001000);
  // Imprimir la imagen en la pantalla
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S4-");
  display.drawCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.fillCircle(90, 28, 7, WHITE);
  display.drawCircle(40, 28, 7, WHITE);
  display.drawCircle(50, 10, 7, WHITE);
  display.drawCircle(80, 10, 7, WHITE);
  display.display();
  if ( digitalRead(12) == 0) {
    delay(a);
    goto S5;
  }
  if ( digitalRead(13) == 0) {
    delay(a);
    goto S55;
  }
  if ( digitalRead(35) == 0) {
    goto S22;
  }
  if ( digitalRead(34) == 0) {
    delay(a);
    goto S3;
  }
  if ( digitalRead(15) == 0) {
    goto S33;
  }
  if ( digitalRead(4)  == 0) {
    goto S33;
  }
  goto S4;
S55:
  delay(a);
S5:
  display.clearDisplay();
  Salidas(0B010000);
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S5-");
  display.drawCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.drawCircle(90, 28, 7, WHITE);
  display.drawCircle(40, 28, 7, WHITE);
  display.drawCircle(50, 10, 7, WHITE);
  display.drawCircle(80, 10, 7, WHITE);
  display.display();
  if ( digitalRead(12) == 0) {
    delay(a);
    goto S6;
  }
  if ( digitalRead(13) == 0) {
    delay(a);
    goto S66;
  }
  if ( digitalRead(35) == 0) {
    goto S44;
  }
  if ( digitalRead(34) == 0) {
    goto S11;
  }
  if ( digitalRead(15) == 0) {
    delay(a);
    goto S6;
  }
  if ( digitalRead(4)  == 0) {
    goto S44;
  }
  goto S5;

S66:
  delay(a);
S6:
  Salidas(0B100000);
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 5);
  display.println("-S6-");
  display.fillCircle(80, 48, 7, WHITE);
  display.drawCircle(50, 48, 7, WHITE);
  display.drawCircle(90, 28, 7, WHITE);
  display.drawCircle(40, 28, 7, WHITE);
  display.drawCircle(50, 10, 7, WHITE);
  display.drawCircle(80, 10, 7, WHITE);
  display.display();
  if ( digitalRead(12) == 0) goto S1;
  if ( digitalRead(13) == 0) {delay(a);goto S11;}
  if ( digitalRead(35) == 0) goto S11;
  if ( digitalRead(34) == 0) goto S22;
  if ( digitalRead(15) == 0) goto S33;
  if ( digitalRead(4)  == 0)  {
    delay(a);
    goto S55;
  }
  goto S6;
}

void Salidas(byte W)
{
  for (int s = 0 ; s < 6; s++) {
    digitalWrite(SALIDAS_PUERTO[s], bitRead(W, s));
  }
}
