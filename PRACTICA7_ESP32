void setup() {
  Serial.begin(9600);
  display.begin(SSD1306_SWITCHCAPVCC);
  display.clearDisplay();
  display.drawBitmap(0  , 10, ABB_MONO, 128, 64, WHITE);
  display.drawBitmap(30 , 5, ABB,      128, 64, WHITE);
  display.display();
  delay (1000);
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(6, 0);
  display.println("-MICROCONTROLADORES-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 10);
  display.println("-HUITZILIHUITL-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(1000);
  display.clearDisplay();
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 0);
  display.println("-JESUS_EMMANUEL-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(8, 10);
  display.println("-JORGE_ALBERTO-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(1000);
  display.clearDisplay();
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(25, 0);
  display.println("-PRACTICA_7-");
  display.display();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(25, 10);
  display.println("-SENS_BAR-");
  display.drawLine(0, 20, 150, 20, WHITE);
  display.display();
  delay(500);
  display.clearDisplay();
  display.display();
}

void loop() {
  Entrada = analogRead(35);
  Entrada2= analogRead(33);
  Entrada3= analogRead(25);
  Serial.println(Largo3);
  Largo    = map(Entrada , 0, 4095, 2, 100);
  Largo2   = map(Entrada2, 0, 4095, 1, 100);
  Largo3   = map(Entrada3, 0, 4095, 0, 100);
  degreesC = map(Entrada2, 0, 4095 , 0, 500 );
  Imprimir(Largo , Largo3 , degreesC, Largo2); // RECTANGULO 1
}
void Imprimir (int Largo, int Largo3 , int degreesC , int Largo2 ) // rectangulo 1
{
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 0);
  display.println("LDR->");
  display.drawRect(30, 0, Largo, 8, WHITE);
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(126, 0);
  display.println(Largo);
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 20);
  display.println("TEMP-");
  display.drawRect(30, 20, Largo2, 8, WHITE);
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(15, 30);
  display.println("C");
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 30);
  display.println(degreesC );
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 40);
  display.println("PRES");
  display.drawRect(30, 40, Largo3, 8, WHITE);
   display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 50);
  display.println(Largo3);
  delay(50);
  display.display();
}
